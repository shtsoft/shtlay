<!DOCTYPE html>
<html lang="en">


  <head>

    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="{{ page.description }}">
 
    <title>
      {{ page.title }}
    </title>

    <link rel="stylesheet" href="/bootstrap.css">
    <link rel="stylesheet" href="/shtlay.css">
    <link rel="icon" href="{{ page.iconfile }}" type="image/x-icon">
    <link rel="canonical" href="{{ page.canonical }}">

  </head>


  <body>

    <nav>
      <a href="{{ page.brandlink }}">
        {{ page.brand }}
      </a>
      <button type="button">
        <span></span>
      </button>
      <ul class="nobullets tablist">
{% if page.navitems %}
  {% render {{ page.navitems }} %}
{% endif %}
      </ul>
    </nav>
  
    <header>
      <div class="container">
        <div class="row">
          <div class="col-lg-9 col-md-10 mx-auto">
            <div class="heading">
              <h1>
                {{ page.heading }}
              </h1>
              <span class="subheading">
                {{ page.subheading }}
              </span>
            </div>
          </div>
        </div>
      </div>
    </header>

    <div id="content">
      <div class="container">
        <div class="row">
          <div class="col-lg-9 col-md-10 mx-auto">
{{ content }}
          </div>
        </div>
      </div>
    </div>

    <div class="footer-separator"></div>

    <footer>
      <ul class="nobullets">
{% if page.footeritems %}
  {% render {{ page.footeritems }} %}
{% endif %}
      </ul>
    </footer>

    <script src="/shtlay.js"></script>
    <script src="/bootstrap.js"></script>

  </body>


</html>
